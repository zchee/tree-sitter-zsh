{
  "grammars": [
    {
      "name": "zsh",
      "camelcase": "Zsh",
      "scope": "source.zsh",
      "path": ".",
      "file-types": [
        "zsh",
        ".zshrc",
        ".zshenv",
        ".zprofile"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^(shell|bash|sh)$",
      "first-line-regex": "^#!.*\\b(sh|bash|dash)\\b.*$"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "ZSH grammar for tree-sitter",
    "authors": [
      {
        "name": "Amaan Qureshi",
        "email": "amaanq12@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/zchee/tree-sitter-zsh"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
